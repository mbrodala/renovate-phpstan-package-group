{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "rangeStrategy": "update-lockfile",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "phpstan/"
      ],
      "matchPackageNames": [
        "jangregor/phpstan-prophecy",
        "saschaegerer/phpstan-typo3",
        "rector/rector"
      ],
      "groupName": "PHPStan"
    }
  ]
}
